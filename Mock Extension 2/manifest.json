{
    "manifest_version":3,
    "name":"NumePay Wallet",
    "version":"0.0.1",
    "description":"This is the mock NumePay Wallet",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js":["content.js"]
        }
    ],
    "background":{
        "service_worker":"background.js"
    },
    "icons":{
        "128":"icon128.png",
        "48":"icon48.png",
        "16":"icon16.png"
    },
    "action":{
        "default_icon":"icon16.png",
        "default_popup":"popup.html"
    },
    "externally_connectable": {
        "ids": ["*"],
        "matches": ["file:///Payasyougo*"]
    },
    "permissions": [
        "storage",
        "tabs"
    ]
    
    
}